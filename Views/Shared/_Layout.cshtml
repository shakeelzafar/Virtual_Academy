<body class="home-page">
    <div class="wrapper">
        <!-- ******HEADER****** -->
        <header class="header">
            <div class="top-bar">
                <div class="container">
                    <ul class="social-icons col-md-6 col-sm-6 col-xs-12 hidden-xs">
                        <li><a href="../../Home/Index#"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="../../Home/Index#"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="../../Home/Index#"><i class="fa fa-youtube"></i></a></li>
                        <li><a href="../../Home/Index#"><i class="fa fa-linkedin"></i></a></li>
                        <li><a href="../../Home/Index#"><i class="fa fa-google-plus"></i></a></li>
                        <li class="row-end"><a href="../../Home/Index#"><i class="fa fa-rss"></i></a></li>
                    </ul><!--//social-icons-->
                   

                   <div>
                    <form class="pull-right search-form" role="search">
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Search the site...">
                        </div>
                        <button type="submit" class="btn btn-theme">Go</button>
                        
                    </form>
                       @Html.Partial("~/Views/Shared/_LoginPartial.cshtml") 
                   </div>
                </div>
            </div><!--//to-bar-->
            <div class="header-main container">
                <h1 class="logo col-md-4 col-sm-4" style="font-size:20px" >
                    <a href="../../Home/Index"><img style="height:60px" id="logo" src="~/about_files/logo.png" alt="Logo" > Virtual Academy</a>
                </h1><!--//logo-->
                <div class="info col-md-8 col-sm-8">
                    <ul class="menu-top navbar-right hidden-xs">
                        <li class="divider"><a href="../../Home/Index">Home</a></li>
                        <li class="divider"><a href="../../Home/FAQ">FAQ</a></li>
                        <li><a href="">Contact</a></li>
                    </ul><!--//menu-top-->
                    <br>
                    <div class="contact pull-right">
                        <p class="phone"><i class="fa fa-phone"></i>Call us today 0800 123 4567</p>
                        <p class="email"><i class="fa fa-envelope"></i><a href="../../Home/Index#">enquires@website.com</a> </p>
                    
                    
                    
                    </div><!--//contact-->
                </div><!--//info-->
            </div><!--//header-main-->
        </header><!--//header-->
        <!-- ******NAV****** -->
        <nav class="main-nav" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button><!--//nav-toggle-->
                </div><!--//navbar-header-->
                <div class="navbar-collapse collapse" id="navbar-collapse">
                    <ul class="nav navbar-nav">
                        <li class="active nav-item"><a href="../../Home/Index">Home</a></li>
                        <li class="nav-item dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="0" data-close-others="false" href="../../Home/Index#">Courses <i class="fa fa-angle-down"></i></a>
                            <ul class="dropdown-menu">
                                @{
                                    db ctx = new db();
                                    List<Class> list = ctx.Class.ToList();
                                    }
                                @foreach(var v in list)
                                {
                                  <li class="dropdown-submenu">
                                    <a class="trigger" tabindex="-1" href="../../Home/Index#">@v.Name<i class="fa fa-angle-right"></i></a>
                                    <ul class="dropdown-menu">
                                        @foreach(var sub in v.SubjectClass.Select(x=>x.Subject))
                                        { 
                                        <li class="dropdown-submenu">
                                            <a   href="../../Class/Subject/@sub.SubjectId">@sub.Name<i class="fa fa-angle-right"></i></a>
                                            
                                        </li>
                                        }
                                    </ul>
                                </li>
                                }
                            </ul>
                        </li>
                        <li class="nav-item"><a href="../../Quiz/Index">Quiz</a></li>
                       
                        <li class="nav-item dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="0" data-close-others="false" href="../../Home/Index#">Pages <i class="fa fa-angle-down"></i></a>
                            <ul class="dropdown-menu">
                                <li><a href="../../Home/About">About</a></li>
                                <li><a href="../../Home/Team">Leadership Team</a></li>
                                <li><a href="http://themes.3rdwavemedia.com/college-green/gallery-album.html">Single Gallery</a></li>
                                <li><a href="../../Home/FAQ">FAQ</a></li>
                                <li><a href="../../Home/PrivatePolicy">Privacy Policy</a></li>
                                <li><a href="">Terms &amp; Conditions</a></li>
                            </ul>
                        </li><!--//dropdown-->
                        <li class="nav-item"><a href="../../Home/Contact">Contact</a></li>
                    </ul><!--//nav-->
                </div><!--//navabr-collapse-->
            </div><!--//container-->
        </nav><!--//main-nav-->
        <!-- ******CONTENT****** -->
    <div>
        @RenderBody()

        @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)
        </div>
    </div><!--//wrapper-->
